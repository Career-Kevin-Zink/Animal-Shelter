                                        <VBox fx:id="adoptableVbox" prefHeight="41.0" prefWidth="245.0"/>
                                    </children>
                                </HBox>
                            </children>
                        </VBox>
                        <Button fx:id="escapeBtn" layoutX="552.0" layoutY="8.0" mnemonicParsing="false"
